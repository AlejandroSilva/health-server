{
  "name": "health-server",
  "version": "1.0.0",
  "description": "nodo heath para el envio de informacion del sistema",
  "main": "index.js",
  "scripts": {
    "start-station-prod": "NODE_ENV=production pm2 start health-station.js -f --interpreter babel-node --name station && pm2 logs station",
    "stop-station-prod": "pm2 stop station",
    "babel-dev": "babel-node health-server.js --source-maps --loglevel error",
    "babel-test": "NODE_ENV=testing babel-node health-server.js --source-maps --watch",
    "mocha-v1-info": "NODE_ENV=testing ./node_modules/.bin/mocha --compilers js:babel/register --timeout 5000 test/v1.info.js ",
    "mocha-v1-server": "NODE_ENV=testing ./node_modules/.bin/mocha --compilers js:babel/register --timeout 8000 test/v1.health-server.js ",
    "test": "npm run mocha-v1-server"
  },
  "author": "toth.cl",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "helmet": "^0.10.0",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "node-schedule": "^0.2.9",
    "react": "^0.13.3",
    "react-dom": "^0.14.0-rc1",
    "react-redux": "^2.1.2",
    "react-router": "1.0.0-rc1",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "request": "^2.61.0",
    "rethinkdb": "^2.1.1",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "superagent": "^1.3.0",
    "tcp-ping": "^0.1.1",
    "thinky": "^2.1.6",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "mocha": "^2.3.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.2",
    "supertest": "^1.1.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
